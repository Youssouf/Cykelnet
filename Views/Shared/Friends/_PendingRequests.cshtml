@model List<Cykelnet.Models.FriendRequest>

@if (Model.Count > 0)
{
    <div style="max-height: 50%;">
        <h1>Friend Requests</h1>
        <ul class="friend-request">
        @foreach (var item in Model)
        {
            <li id="friend-request-@item.FromUser">
                @{Html.RenderPartial("Friends/_Request", item);}
	        </li>
        }
        </ul>
        <div style="clear:both"></div>
    </div>
}